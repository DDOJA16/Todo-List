{
  "singleQuote": true,
  "trailingComma": "all",
  "prettier/prettier": [
    "error",
    {
      "singleQuote": true,
      "parser": "flow",
      "printWidth": 80,
      "tabWidth": 2,
      "useTabs": false,
      "semi": true,
      "quoteProps": "consistent",
      "trailingComma": "es5",
      "bracketSpacing": true,
      "arrowParens": "always",
      "endOfLine": "lf"
    }
  ]
}
